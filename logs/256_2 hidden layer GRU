cfg['dropout'] = 0.005 # dropout rate between two layers of GRU; useful only when layers > 1; between 0 and 1
cfg['bidirectional'] = True # True or False; True means using a bidirectional GRU
cfg['batch_size'] = 120 # batch size of input
cfg['learning_rate'] = 2e-4 # learning rate to be used
